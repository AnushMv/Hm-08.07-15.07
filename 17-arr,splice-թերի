function getSequence(arr){
    let testArr = [];
    for(let i = 0; i < arr.length; i++){
        if(arr[i] != arr[i+1] - 1){
            testArr.push(arr[i]);
            arr.splice(arr[i], 0, arr[i] + 1);
            arr.shift();
        } else {
            testArr.push(arr[i]);
            testArr.push(arr[i+1]);
            arr.shift();
        }
        
    }
    return testArr;
}
